void main()
{
  int valor;
  valor = suma(10,20,30);
  System.out.println(valor);
  
  coche miCoche = new coche();
  System.out.println(miCoche.npuertas);
 }



int suma(int a,int b, int c){
int resultado = a+b+c;
return resultado;
}


class coche{
  int npuertas;

  void masPuertas(){
    this.npuertas++;
  }
}
